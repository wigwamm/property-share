<% if @image.errors.any? %>
  console.log("<%= @image.errors %>")
  $('#upload_<%=params[:unique_id]%>').hide();
<% else %>
  console.log("image added")
  console.log($('ul#property_images'))
  $('ul#property_images').prepend("<%= escape_javascript( render(:partial => 'images/image', locals: {image: @image}) ) %>")
<% end %>